{
  "name": "svglb",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.4.0",
  "description": "cli app for creating SVG icon libraries",
  "author": "Zdorovtsev Viktor",
  "license": "MIT",
  "funding": "https://github.com/sponsors/blasdfaa",
  "homepage": "https://github.com/blasdfaa/svglb#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blasdfaa/svglb.git"
  },
  "bugs": "https://github.com/blasdfaa/svglb/issues",
  "keywords": [
    "svg",
    "cli",
    "generator",
    "icons",
    "package"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "svglb": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "execa": "^9.3.0",
    "giget": "^1.2.3",
    "nypm": "^0.3.9",
    "pathe": "^1.1.2",
    "prettier": "^3.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.22.0-beta.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^20.14.10",
    "bumpp": "^9.4.1",
    "eslint": "^9.6.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.7",
    "pnpm": "^9.4.0",
    "rimraf": "^5.0.8",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3",
    "unbuild": "^2.0.0",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
